    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.10.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "es6-promise": "^4.2.8",
    "express": "^4.17.1",
